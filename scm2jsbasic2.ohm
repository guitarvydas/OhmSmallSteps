Scm2JSBasic {
  Scm = List | Atom |  Boolean
  QuotedSexp = "'" Scm
  BackQuoteSexp = "`" Scm
  CommaSexp = "," Scm
  List =   "(" ListItem+ ")"
  ListItem = (Atom | List)
  Atom = Integer | Symbol | Boolean | NullList | QuotedSexp | BackQuoteSexp | CommaSexp
  NullList = "(" ")"
  Boolean = "#f" | "#t"
  Integer = Numchar+
  Numchar = "0".."9"
  Symbol = Letchar (Letchar | Numchar | "?" | "_" | "-" | "!")*
  Letchar = LC | UC | "+" | "*"
  LC = "a".."z"
  UC = "A".."Z"
}
